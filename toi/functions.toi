recursion|a| {
    println("recursion", a)
    if a > 0 {
        recursion(a - 1)
    }
}

recursion(3)

forwardCallWorks|s| {
    println(hello(s))
}

hello|s| return {
    return = "Hello, " _ s
}

forwardCallWorks("forward call")
