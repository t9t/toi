lines = inputLines()
lineCount = len(lines)

answers = map()
sum = 0

i = 0
while i <= lineCount {
    if i == lineCount {
        line = ""
    } otherwise {
        line = [lines]i
    }
    i = i + 1

    if line == "" {
        sum = sum + len(answers)
        answers = map()
    } otherwise {
        for c = [chars(line)]ci {
            [answers]c = 1
        }
    }
}

println(sum)
