lines = inputLines()
lineCount = len(lines)

answers = map()
sum = 0

i = 0
while i <= lineCount {
    if i == lineCount {
        line = ""
    } otherwise {
        line = [lines]i
    }
    i = i + 1

    if line == "" {
        sum = sum + len(answers)
        answers = map()
    } otherwise {
        chars = chars(line)
        ci = 0
        while ci < len(chars) {
            c = [chars]ci
            ci = ci + 1

            set(answers, c, 1)
        }
    }
}

println(sum)
