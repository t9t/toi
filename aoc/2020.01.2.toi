numbers = inputNumbers()
i = len(numbers)

while i <> 0 {
    i = i - 1
    left = get(numbers, i)

    j = i
    while j <> 0 {
        j = j - 1
        middle = get(numbers, j)

        k = j
        while k <> 0 {
            k = k - 1
            right = get(numbers, k)

            if 2020 - left + middle + right == 0 {
                println(2020, left, middle, right, left * middle * right)
            }
        }
    }
}
