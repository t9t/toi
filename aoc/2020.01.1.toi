Set numbers to: inputNumbers().
Set i to: len(numbers).

Loop while i is not equal to 0:
    Set i to: i - 1.
    Set left to: get(numbers, i).

    Set j to: i.
    Loop while j is greater than 0:
        Set j to: j - 1.
        Set right to: get(numbers, j).

        if left + right is equal to 2020:
            println(2020, left, right, left * right).
        .
    .
.
